    <icon href="http://betaville.net/webstart/BetavilleAppIcon.gif"/> 
    <association extensions="bxb" mime-type="text/plain"/>
    <shortcut online="false">
  	<desktop />
  	<menu submenu="Brooklyn eXperimental Media Center"/>
  </shortcut>
  </information>
  <security>
      <all-permissions/>
  </security>
  <resources>
