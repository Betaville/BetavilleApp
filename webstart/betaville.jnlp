<property key="java.library.path" value="." />

<resources os="Windows">
  	<j2se version="1.5+" max-heap-size="768M"/>
    <nativelib href="windows-native.jar"/>
  </resources>
  <resources os="Mac OS">
    <j2se version="1.5+" max-heap-size="768M"/>
    <nativelib href="mac-native.jar"/>
  </resources>
  <resources os="Linux" arch="i386">
    <j2se version="1.5+" max-heap-size="768M"/>
    <nativelib href="linux-native.jar"/>
  </resources>

  <application-desc main-class="edu.poly.bxmc.betaville.jme.BetavilleNoCanvas"/> 
  <component-desc/>
</jnlp>