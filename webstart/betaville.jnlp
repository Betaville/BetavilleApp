<?xml version="1.0" encoding="utf-8"?>
<jnlp spec="1.0+"
      codebase="http://betaville.net/webstart"
      href="betaville.jnlp">
 
  <information>
    <title>Betaville</title>
    <vendor>BxmC</vendor>
    <homepage href="http://bxmc.poly.edu"/>
